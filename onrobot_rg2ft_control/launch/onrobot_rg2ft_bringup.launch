<launch>
    <arg name="ip" default="192.168.1.1" />
    <arg name="port" default="502" />

    <node name="onrobot_rg2ft_driver"
        pkg="onrobot_rg2ft_control"
        type="OnRobotRG2FTDriver.py"
        output="screen">

        <param name="ip"               value="$(arg ip)" />
        <param name="port"             value="$(arg port)" />

    </node>
</launch>